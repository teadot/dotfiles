Host ssh.dev.azure.com
	IdentityFile ~/.ssh/id_rsa_azure_devops

Host github.com
	IdentityFile ~/.ssh/github_id_rsa


# PACKERAGENTS
Match exec "grepcidr 10.146.13.160/28 <(echo %h) &>/dev/null"
	User adminuser
	IdentityFile ~/.ssh/id_rsa_az5_packeragents
	StrictHostKeyChecking no
	
# DEVAGENTS
Match exec "grepcidr 10.146.13.96/27 <(echo %h) &>/dev/null"
	User adminuser
	IdentityFile ~/.ssh/id_rsa_az5_devagents
	StrictHostKeyChecking no
	
